{
  "name": "did-jwt-toolkit",
  "version": "0.1.0",
  "description": "The toolkit for did-jwt and did-jwt-vc",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "LICENSE"
